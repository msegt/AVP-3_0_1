<?xml version="1.0"?>
<configuration debug="true">
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>
        [%thread] %-5level %logger{36} - %msg%n
     </pattern>
    </encoder>
  </appender>
  <root level="WARN">
    <appender-ref ref="STDOUT"/>
  </root>
  <logger name="hmi.flipper2" level="INFO"/>
  <logger name="eu.aria.dm.managers" level="INFO"/>
  <logger name="eu.aria.dm.behaviours" level="INFO"/>
</configuration>