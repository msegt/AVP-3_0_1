	<face id="$(0{select*168=1;precision=0})">
		<emotion>
			<neutral>$(2{select*7=0;precision=2})</neutral>		
			<anger>$(2{select*7=1;precision=2})</anger>		
			<disgust>$(2{select*7=2;precision=2})</disgust>
